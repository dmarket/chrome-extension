{
  "overrides": [
    {
      "files": "*.component.html",
      "options": {
        "parser": "angular"
      }
    }
  ],
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-sort-json",
    "prettier-plugin-tailwindcss"
  ],
  "printWidth": 100,
  "singleAttributePerLine": true,
  "singleQuote": true
}
